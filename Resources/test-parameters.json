{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appGatewayName": {
            "value": "dfc-foo-bar-ag"
        },
        "appGatewaySubnetRef": {
            "value": "/subscriptions/12345678-1234-5678-9abc-123456789abc/resourceGroups/dfc-foo-bar-rg/providers/Microsoft.Network/virtualNetworks/dfc-foo-bar-vnet/subnets/appgateway"
        },
        "appGatewayPools": {
            "value": [ {
                "name": "mypool",
                "fqdn": "foo.example.net"
            } ]
        },
        "appGatewayPoolHttpSettings": {
            "value": [ {
                "name": "myHttpSetting",
                "port": 80,
                "protocol": "Http"
            } ]
        },
        "appGatewayRouting": {
            "value": [ {
                "name": "myrule",
                "backend": "mypool",
                "paths": [ "/dummy/*" ]
            } ]
        },
        "appGatewayRewrites": {
            "value": [
                {
                    "name": "RWR1",
                    "actionSet": {
                        "requestHeaderConfigurations": [ {
                            "headerName": "Accept-Charset",
                            "headerValue": "utf-8"
                        } ],
                        "responseHeaderConfigurations": [ {
                            "headerName": "Content-Type",
                            "headerValue": "text/html; charset=utf-8"
                        } ]
                    }
                }
            ]
        }
    }
}