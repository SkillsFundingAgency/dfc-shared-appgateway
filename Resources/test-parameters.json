{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourcePrefix": {
            "value": "dfc-foo-shared"
        },
        "virtualNetworkAddressPrefix": {
            "value": "10.10"
        },
        "appGatewayPools": {
            "value": [ {
                "name": "mypool",
                "fqdn": "foo.example.net"
            } ]
        },
        "appGatewayRules": {
            "value": [ {
                "name": "myrule",
                "pool": "mypool",
                "paths": [ "/dummy/*" ]
            } ]
        }
    }
}