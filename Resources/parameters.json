{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "resourcePrefix": {
            "value": "__AzurePrefix__"
        },
        "virtualNetworkAddressPrefix": {
            "value": "__networkPrefix__"
        },
        "appGatewayPools": {
            "value": [ {
                "name": "defaultBackend",
                "fqdn": "__liveBackend__"
            }, {
                "name": "oldBackend",
                "fqdn": "__bauBackend__"
            } ]
        },
        "appGatewayRules": {
            "value": [ {
                "name": "defaultUrlMapping",
                "backend": "oldBackend",
                "paths": [
                    "/skills-health-check/*",
                    "/ResourcePackages/*",
                    "/your-account/*",
                    "/home/signout"
                ]
            } ]
        },
        "publicDnsLabel": {
            "value": "__publicDnsLabel__"
        }
    }
}