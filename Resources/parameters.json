{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appGatewayName": {
            "value": "__appGatewayName__"
        },
        "appGatewaySubnetRef": {
            "value": "__subnetResourceId__"
        },
        "appGatewayPools": {
            "value": [ {
                "name": "defaultBackend",
                "fqdn": "__liveBackend__"
            }, {
                "name": "oldBackend",
                "fqdn": "__bauBackend__"
            } ]
        },
        "appGatewayRouting": {
            "value": [ {
                "name": "defaultUrlMapping",
                "backend": "oldBackend",
                "paths": [
                    "/skills-health-check/*",
                    "/ResourcePackages/*",
                    "/your-account/*",
                    "/home/signout"
                ]
            } ]
        },
        "appGatewayRewrites": {
            "value": [ {
                "name": "RWR1",
                "rules": [ {
                    "name": "Charset",
                    "actionSet": {
                        "requestHeaderConfigurations": [ {
                            "headerName": "Accept-Charset",
                            "headerValue": "utf-8"
                        } ],
                        "responseHeaderConfigurations": [ {
                            "headerName": "Content-Type",
                            "headerValue": "text/html; charset=utf-8"
                        } ]
                    }
                } ]
            } ]
        },
        "publicDnsLabel": {
            "value": "__publicDnsLabel__"
        },
        "keyVaultName": {
            "value": "__keyVaultName__"
        },
        "keyVaultSecretName": {
            "value": "__keyVaultSecretName__"
        }
    }
}