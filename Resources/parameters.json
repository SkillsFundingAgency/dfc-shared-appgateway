{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appGatewayName": {
            "value": "__appGatewayName__"
        },
        "appGatewaySubnetRef": {
            "value": "__subnetResourceId__"
        },
        "appGatewayPools": {
            "value": [ {
                "name": "defaultBackend",
                "fqdn": "__liveBackend__"
            }, {
                "name": "oldBackend",
                "fqdn": "__bauBackend__"
            } ]
        },
        "appGatewayRules": {
            "value": [ {
                "name": "defaultUrlMapping",
                "backend": "oldBackend",
                "paths": [
                    "/skills-health-check/*",
                    "/ResourcePackages/*",
                    "/your-account/*",
                    "/home/signout"
                ]
            } ]
        },
        "publicDnsLabel": {
            "value": "__publicDnsLabel__"
        }
    },
    "keyVaultName": {
        "value": "__keyVaultName__"
    },
    "keyVaultSecretName": {
        "value": "__keyVaultSecretName__"
    }
}